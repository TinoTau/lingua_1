{
  "separator": {
    "default": " ^^ ",
    "translations": [
      " ^^ ",
      "^^",
      " ^^",
      "^^ "
    ],
    "word_variants": []
  },
  "text_filter": {
    "punctuation_only_filter": {
      "enabled": true,
      "description": "过滤只包含标点符号的翻译结果（如 '试试试' 被翻译成 'try.' 的情况）",
      "regex_pattern": "[^\\w\\u4e00-\\u9fff]",
      "min_text_length_after_filter": 1
    }
  }
}

